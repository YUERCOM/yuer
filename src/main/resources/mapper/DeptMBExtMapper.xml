<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuer.mapper.DeptMBMapper">
  <select id="selectDeptInfoByIds" resultType="com.yuer.model.DeptMB">
  	select yuerd.id as id,yuerd.name as name from yuer_dept yuerd where yuerd.id in (${deptIds}) 
  </select>
</mapper>